{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [{
        "label": "run",
        "command": "node index.js",
        "args": ["--initialpopulation=500", "--generations=500"],
        "type": "shell",
        "presentation": {
            "reveal": "always"
        },
        "problemMatcher": "$msCompile",
        "options": {
            "cwd": "${workspaceRoot}",
            "shell": {
                "executable": "cmd.exe",
                "args": ["/d", "/c"]
            }
        }
    }]
}